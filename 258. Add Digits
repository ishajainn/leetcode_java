class Solution {
    public int addDigits(int num) {
        if(num>9){
        while(num>9){
            int num2=num;
            int sum=0;
            while(num2>0){
                int nes=num2%10;
                sum+=nes;
                num2/=10;
            }
            num=sum;
        }
        return num; }
        else{
            return num;
        }
    }
}
