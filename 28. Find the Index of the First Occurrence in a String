class Solution {
   
    public int strStr(String haystack, String needle) {
        int c=-1;
        for(int i=0;i<haystack.length()-needle.length()+1;i++){
            
            String s=haystack.substring(i,i+needle.length());
            if(s.equals(needle)){
                c=i;
                break;
                }
        }
        
      return c;
    }
}
