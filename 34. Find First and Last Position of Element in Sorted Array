class Solution {
    public int[] searchRange(int[] nums, int target) {
        int[] ans = new int[2];
        int start = -1;
        int end = -1;
        
        for(int i=0; i<nums.length; i++){
            if(nums[i]==target){
                if(start == -1){
                    start = i; 
                }
                end = i;
            }
            
            if(nums[i]>target){
                break;
            }
        }
        
        
        ans[0] = start;
        ans[1] = end;
        return ans;
    }
}
