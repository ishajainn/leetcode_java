class Solution {
    public int removeElement(int[] nums, int val) {
        int arr[]=new int[nums.length];
        int count=0;
        int j=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]!=val){
                    nums[count++] = nums[i];
                    j++;}
        }
        return j;
    }}
