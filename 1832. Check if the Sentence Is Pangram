class Solution {
    public boolean checkIfPangram(String sentence) {
          HashSet<String> set =new HashSet<>();
        for(int i=0;i<sentence.length();i++){
            set.add(Character.toString(sentence.charAt(i)));
        }
        boolean c=true;
        if(set.size()!=26)
            c=false;
        return c;
        
    }
}
