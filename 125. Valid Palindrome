class Solution {
    public boolean isPalindrome(String s) {
        s=s.toLowerCase();
        String str= s.replaceAll("[^A-Za-z0-9]+", "");
        System.out.println(str);
        int num=str.length()-1;
        boolean c=true;
        for(int j=0;j<str.length();j++){
            if(str.charAt(j)!=str.charAt(num)){
                System.out.println(str.charAt(j));
                return false;
            }

            num--;}return true;
   
    
    }}
