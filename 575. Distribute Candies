class Solution {
    public int distributeCandies(int[] candyType) {
        HashSet<Integer> set = new HashSet<>();
        for(int ele:candyType){
            set.add(ele);
        }
        int num=candyType.length/2;
        if(set.size()>num){
            return num;
        }
        else{
            return set.size();
        }

        
    }
}
